    "@tailwindcss/vite": "^4.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.6.1",
    "astro-remote": "^0.3.4",
    "elkjs": "^0.10.0",
    "force-graph": "^1.50.1",
    "matchina": "file:..",
    "mermaid": "^11.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reactflow": "^11.11.4",
    "sharp": "^0.34.2",
    "starlight-typedoc": "^0.21.3",
