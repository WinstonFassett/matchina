{
  "name": "matchina",
  "version": "0.1.1",
  "author": "Winston Fassett",
  "repository": "WinstonFassett/matchina",
  "homepage": "https://github.com/WinstonFassett/matchina#readme",
  "license": "MIT",
  "keywords": [],
  "bugs": {
    "url": "https://github.com/WinstonFassett/matchina/issues"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.ts",
      "require": "./dist/index.ts"
    },
    "./react": {
      "types": "./dist/integrations/react.d.ts",
      "import": "./dist/integrations/react.ts",
      "require": "./dist/integrations/react.ts"
    },
    "./zod": {
      "types": "./dist/integrations/zod.d.ts",
      "import": "./dist/integrations/zod.ts",
      "require": "./dist/integrations/zod.ts"
    },
    "./valibot": {
      "types": "./dist/integrations/valibot.d.ts",
      "import": "./dist/integrations/valibot.ts",
      "require": "./dist/integrations/valibot.ts"
    },
    "./inspect": {
      "types": "./dist/inspect/index.d.ts",
      "import": "./dist/inspect/index.ts",
      "require": "./dist/inspect/index.ts"
    },
    "./viz": {
      "types": "./dist/viz/index.d.ts",
      "import": "./dist/viz/index.ts",
      "require": "./dist/viz/index.ts"
    },
    "./hsm": {
      "types": "./dist/hsm/index.d.ts",
      "import": "./dist/hsm/index.ts",
      "require": "./dist/hsm/index.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "workspaces": [
    ".",
    "docs"
  ],
  "scripts": {
    "build": "npm run build:lib",
    "build:lib": "unbuild && size-limit",
    "test-build": "npm run test:types && npm --workspace docs run check",
    "build:docs": "npm --workspace docs run build",
    "build:all": "npm run build:lib && npm run build:docs",
    "dev": "vitest dev",
    "dev:all": "npm run dev && npm --workspace docs run dev",
    "dev:docs": "npm run dev:prepare && npm --workspace docs run dev",
    "dev:prepare": "unbuild --stub",
    "coverage": "vitest run --coverage",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test docs",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test docs -w",
    "release": "npm run  test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run --coverage",
    "test:all": "npm run test:types && npm run test",
    "test:types": "tsc --noEmit --skipLibCheck",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "dry-run": "npm run build && npm test && npx changelogen --dry-run && npm publish --dry-run",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^12.0.0",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^4.0.16",
    "changelogen": "^0.5.0",
    "eslint": "^8.51.0",
    "eslint-config-unjs": "^0.2.1",
    "prettier": "^3.6.2",
    "size-limit": "^10.0.1",
    "unbuild": "^3.6.1",
    "valibot": "^1.2.0",
    "vitest": "^4.0.16",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@astrojs/ts-plugin": "^1.10.4"
  },
  "peerDependencies": {
    "immer": "^10.1.1",
    "valibot": "^1.0.0",
    "zod": "^3.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "force-graph": "^1.51.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "force-graph": {
      "optional": true
    }
  }
}
